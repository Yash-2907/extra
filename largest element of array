#include<stdio.h>
int main()
{
    int i,j,n,large;
    printf("pls enter the length of array :- ");
    scanf("%d",&n);
    int arr[n];
    printf("pls enter the terms of array:-\n");
    for (i=0;i<n;i++)
    {
      scanf("%d",&arr[i]);
    }
    large=arr[0];
    for(j=0;j<n;j++)
    {
        if(large<arr[j])
        {
          large=arr[j];
        }
    }
    printf("LARGEST ELEMENT OF ENTETRED ARRAY IS :-%d",large);
    return 0;

}
